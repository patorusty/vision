></v-spacer>\n      <v-dialog\n        @click:outside=\"HIDE_MODAL2(false)\"\n        :value=\"modal2\"\n        max-width=\"40%\"\n      >\n        <modal-endosos />\n      </v-dialog>\n    </v-card-title>\n    <v-data-table\n      class=\"pa-2\"\n      :headers=\"headers\"\n      :items-per-page=\"5\"\n      :items=\"tableData\"\n      :search=\"search\"\n      multi-sort\n      :loading=\"loading\"\n    >\n      <template v-slot:[`item.tipo`]=\"{ item }\">\n        {{\n          item.tipo_endoso.nombre\n        }}\n      </template>\n      <template v-slot:[`item.asegurado`]=\"{ item }\">\n        {{\n          nombreCompleto(item.poliza.cliente)\n        }}\n      </template>\n      <template v-slot:[`item.compania`]=\"{ item }\">\n        {{\n          item.poliza.compania.nombre\n        }}\n      </template>\n      <template v-slot:[`item.fecha_solicitud`]=\"{ item }\">\n        {{\n          formatDate(item.fecha_solicitud)\n        }}\n      </template>\n      <template v-slot:[`item.actions`]=\"{ item }\">\n        <v-icon\n          small\n          @click=\"editEndoso(item.id)\"\n          class=\"mr-2\"\n          color=\"success\"\n        >\n          mdi-pencil\n        </v-icon>\n        <v-icon\n          class=\"ml-2\"\n          small\n          @click=\"openDeleteModal(item.id)\"\n          color=\"error\"\n        >\n          mdi-close\n        </v-icon>\n      </template>\n    </v-data-table>\n    <v-dialog\n      :retain-focus=\"false\"\n      max-width=\"30%\"\n      v-model=\"modalDelete\"\n    >\n      <v-card class=\"pa-4\">\n        <v-card-text>\n          <span>Esta seguro que desea eliminar este Endoso?</span>\n        </v-card-text>\n        <v-card-actions class=\"py-0 pt-3 pr-6 d-flex justify-end\">\n          <v-btn\n            dark\n            color=\"red\"\n            @click=\"modalDelete = false\"\n          >Cancelar</v-btn>\n          <v-btn\n            class=\"ml-4\"\n            dark\n            color=\"success\"\n            @click=\"deleteEnd\"\n          >Confirmar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-card>\n</template>\n\n<script>\nimport { mapState, mapActions, mapMutations } from \"vuex\";\nimport ModalEndosos from \"./ModalEndosos\";\nimport { helpers } from \"../../../helpers\";\n\nexport default {\n  components: {\n    ModalEndosos\n  },\n  mixins: [helpers],\n  data: () => ({\n    search: \"\",\n    idSelected: \"\",\n    modalDelete: false,\n    tipo_endoso_id: 1\n  }),\n  computed: {\n    ...mapState(\"modal\", [\"modal2\", \"edicion2\"]),\n    ...mapState(\"endoso\", [\"endosos\", \"tipo_endosos\", \"loading\"]),\n    headers() {\n      return [\n        { text: \"Póliza Nro.\", value: \"poliza.numero\" },\n        { text: \"Tipo\", value: \"tipo\" },\n        { text: \"Asegurado\", value: \"asegurado\" },\n        { text: \"Compañia\", value: \"compania\" },\n        { text: \"F. Solicitud\", value: \"fecha_solicitud\" },\n        { text: \"Actions\", value: \"actions\", sortable: false, align: \"right\" }\n      ];\n    },\n    tableData() {\n      return this.endosos.filter(item =>\n        this.tipo_endoso_id != 0\n          ? item.tipo_endoso_id === this.tipo_endoso_id\n          : item.tipo_endoso_id != 0\n      );\n    },\n    tipoEndosos() {\n      var r = [...this.tipo_endosos, { id: 0, nombre: \"TODOS\" }];\n      return r;\n    }\n  },\n  methods: {\n    ...mapActions(\"endoso\", [\n      \"getEndosos\",\n      \"getEndoso\",\n      \"deleteEndoso\",\n      \"getTipoEndosos\",\n      \"getDetalleEndosos\"\n    ]),\n    ...mapMutations(\"modal\", [\"SHOW_MODAL2\", \"HIDE_MODAL2\"]),\n    editEndoso(id) {\n      this.getEndoso(id);\n      this.SHOW_MODAL2(true);\n    },\n    openDeleteModal(id) {\n      this.idSelected = id;\n      this.modalDelete = true;\n    },\n    deleteEnd() {\n      this.deleteEndoso(this.idSelected);\n      this.modalDelete = false;\n      this.idSelected = \"\";\n    }\n  },\n  created() {\n    this.getEndosos();\n    this.getTipoEndosos();\n    this.getDetalleEndosos();\n  }\n};\n</script>\n\n<style>\n</style>"]}]}