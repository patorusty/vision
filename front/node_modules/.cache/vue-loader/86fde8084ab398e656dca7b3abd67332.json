      @click:outside=\"HIDE_MODAL(false)\"\n        :value=\"modal\"\n        max-width=\"70%\"\n      >\n        <modal-productores></modal-productores>\n      </v-dialog>\n    </v-card-title>\n    <v-data-table\n      class=\"pa-2\"\n      :headers=\"headers\"\n      :items-per-page=\"5\"\n      :items=\"productores\"\n      :search=\"search\"\n      multi-sort\n      :loading=\"loading\"\n    >\n      <template\n        slot=\"item.activo\"\n        slot-scope=\"props\"\n      >{{\n        textoActivo(props.item.activo)\n      }}</template>\n      <template v-slot:[`item.actions`]=\"{ item }\">\n        <v-icon\n          small\n          class=\"mr-2\"\n          @click=\"editProductor(item.id)\"\n          color=\"success\"\n        >\n          mdi-pencil\n        </v-icon>\n        <v-icon\n          class=\"ml-2\"\n          small\n          @click=\"openDeleteModal(item.id)\"\n          color=\"error\"\n        >\n          mdi-close\n        </v-icon>\n      </template>\n    </v-data-table>\n    <v-dialog\n      :retain-focus=\"false\"\n      max-width=\"30%\"\n      v-model=\"modalDelete\"\n    >\n      <v-card class=\"pa-4\">\n        <v-card-text>\n          <span>Esta seguro que desea eliminar este Productor?</span>\n        </v-card-text>\n        <v-card-actions class=\"py-0 pt-3 pr-6 d-flex justify-end\">\n          <v-btn\n            dark\n            color=\"red\"\n            @click=\"modalDelete = false\"\n          >Cancelar</v-btn>\n          <v-btn\n            class=\"ml-4\"\n            dark\n            color=\"success\"\n            @click=\"deleteProd\"\n          >Confirmar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-card>\n</template>\n<script>\nimport { mapState, mapActions, mapMutations } from \"vuex\";\nimport { helpers } from \"../../../../helpers\";\nimport ModalProductores from \"./ModalProductores\";\n\nexport default {\n  components: {\n    ModalProductores\n  },\n  mixins: [helpers],\n  data() {\n    return {\n      search: \"\",\n      idSelected: null,\n      modalDelete: false,\n      headers: [\n        { text: \"Apellido\", value: \"apellido\" },\n        { text: \"Nombre\", value: \"nombre\" },\n        { text: \"CUIT\", value: \"cuit\" },\n        { text: \"E-mail\", value: \"email\" },\n        { text: \"Celular\", value: \"telefono_2\" },\n        { text: \"Activo\", value: \"activo\" },\n        { text: \"Acciones\", value: \"actions\", sortable: false, align: \"right\" }\n      ]\n    };\n  },\n  computed: {\n    ...mapState(\"productor\", [\"productores\", \"loading\"]),\n    ...mapState(\"modal\", [\"modal\"])\n  },\n  methods: {\n    ...mapActions(\"productor\", [\n      \"getProductores\",\n      \"getProductor\",\n      \"deleteProductor\"\n    ]),\n    ...mapMutations(\"modal\", [\"SHOW_MODAL\", \"HIDE_MODAL\"]),\n    editProductor(nombre) {\n      this.getProductor(nombre);\n      this.SHOW_MODAL(true);\n    },\n    openDeleteModal(id) {\n      this.idSelected = id;\n      this.modalDelete = true;\n    },\n    deleteProd() {\n      this.deleteProductor(this.idSelected);\n      this.modalDelete = false;\n      this.idSelected = \"\";\n    },\n    textoActivo(nro) {\n      return nro === 1 ? \"Activo\" : \"Inactivo\";\n    }\n  },\n  created() {\n    this.getProductores();\n  }\n};\n</script>"]}]}