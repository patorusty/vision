%\"\n      >\n        <modal-marca></modal-marca>\n      </v-dialog>\n    </v-card-title>\n    <v-data-table\n      class=\"pa-2\"\n      :headers=\"headers\"\n      :items-per-page=\"5\"\n      :items=\"marcas\"\n      :search=\"search\"\n      multi-sort\n      :loading=\"loading\"\n    >\n      <template v-slot:[`item.actions`]=\"{ item }\">\n        <v-icon\n          small\n          class=\"mr-2\"\n          @click=\"editMarca(item.id)\"\n          color=\"success\"\n        >\n          mdi-pencil\n        </v-icon>\n        <v-icon\n          class=\"ml-2\"\n          small\n          @click=\"openDeleteModal(item.id)\"\n          color=\"error\"\n        >\n          mdi-close\n        </v-icon>\n      </template>\n    </v-data-table>\n    <v-dialog\n      :retain-focus=\"false\"\n      max-width=\"30%\"\n      v-model=\"modalDelete\"\n    >\n      <v-card class=\"pa-4\">\n        <v-card-text>\n          <span>Esta seguro que desea eliminar esta Marca?</span>\n        </v-card-text>\n        <v-card-actions class=\"py-0 pt-3 pr-6 d-flex justify-end\">\n          <v-btn\n            dark\n            color=\"red\"\n            @click=\"modalDelete = false\"\n          >Cancelar</v-btn>\n          <v-btn\n            class=\"ml-4\"\n            dark\n            color=\"success\"\n            @click=\"deleteMarc\"\n          >Confirmar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-card>\n</template>\n\n<script>\nimport { mapState, mapActions, mapMutations } from \"vuex\";\n// import { helpers } from '../../../../helpers';\nimport ModalMarca from \"./ModalMarca\";\nexport default {\n  components: { ModalMarca },\n  data: () => ({\n    search: \"\",\n    modalDelete: false,\n    headers: [\n      { text: \"Nombre\", value: \"nombre\" },\n      { text: \"Acciones\", value: \"actions\", sortable: false, align: \"right\" }\n    ],\n    idSelected: null\n  }),\n  computed: {\n    ...mapState(\"modal\", [\"modal\"]),\n    ...mapState(\"marca\", [\"marcas\", \"loading\"])\n  },\n  methods: {\n    ...mapMutations(\"modal\", [\"SHOW_MODAL\", \"HIDE_MODAL\"]),\n    ...mapActions(\"marca\", [\"getMarca\", \"deleteMarca\"]),\n    editMarca(id) {\n      this.getMarca(id);\n      this.SHOW_MODAL(true);\n    },\n    openDeleteModal(id) {\n      this.idSelected = id;\n      this.modalDelete = true;\n    },\n    deleteMarc() {\n      this.deleteMarca(this.idSelected);\n      this.modalDelete = false;\n      this.idSelected = \"\";\n    }\n  }\n};\n</script>\n\n<style>\n</style>"]}]}