L(false)\"\n        :value=\"modal\"\n        max-width=\"40%\"\n      >\n        <modal-riesgo-automotor />\n      </v-dialog>\n    </v-card-title>\n    <v-data-table\n      class=\"pa-2\"\n      :headers=\"headers\"\n      :items-per-page=\"5\"\n      :items=\"riesgo_automotores\"\n      :search=\"search\"\n      multi-sort\n      :loading=\"loading\"\n    >\n      <template v-slot:[`item.actions`]=\"{ item }\">\n        <v-icon\n          small\n          @click=\"editCodigoOrganizador(item.id)\"\n          class=\"mr-2\"\n          color=\"success\"\n        >\n          mdi-pencil\n        </v-icon>\n        <v-icon\n          class=\"ml-2\"\n          small\n          @click=\"openDeleteModal(item.id)\"\n          color=\"error\"\n        >\n          mdi-close\n        </v-icon>\n      </template>\n    </v-data-table>\n    <v-dialog\n      :retain-focus=\"false\"\n      max-width=\"30%\"\n      v-model=\"modalDelete\"\n    >\n      <v-card class=\"pa-4\">\n        <v-card-text>\n          <span>Esta seguro que desea eliminar este Riesgo?</span>\n        </v-card-text>\n        <v-card-actions class=\"py-0 pt-3 pr-6 d-flex justify-end\">\n          <v-btn\n            dark\n            color=\"red\"\n            @click=\"modalDelete = false\"\n          >Cancelar</v-btn>\n          <v-btn\n            class=\"ml-4\"\n            dark\n            color=\"success\"\n            @click=\"deleteRiesgoAut\"\n          >Confirmar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-card>\n</template>\n\n<script>\nimport { mapState, mapActions, mapMutations } from \"vuex\";\nimport ModalRiesgoAutomotor from \"./Automotor/ModalRiesgoAutomotor\";\nimport { helpers } from \"../../../../helpers\";\n\nexport default {\n  components: {\n    ModalRiesgoAutomotor\n  },\n  mixins: [helpers],\n  data: () => ({\n    search: \"\",\n    idSelected: \"\",\n    modalDelete: false\n  }),\n  computed: {\n    ...mapState(\"modal\", [\"modal\"]),\n    headers() {\n      return [\n        { text: \"Tipo\", value: \"riesgo_automotor.tipo\" },\n        { text: \"Marca\", value: \"riesgo_automotor.marca\" },\n        { text: \"Patente\", value: \"riesgo_automotor.patente\" },\n        {\n          text: \"Suma Asegurada\",\n          value: \"riesgo_automotor.valor_total\"\n        },\n        { text: \"Cobertura\", value: \"riesgo_automotor.cobertura.nombre\" },\n        { text: \"Actions\", value: \"actions\", sortable: false, align: \"right\" }\n      ];\n    }\n  },\n  methods: {\n    ...mapActions(\"riesgo_automotor\", [\n      \"getRiesgoAutomotor\",\n      \"deleteRiesgoAutomotor\"\n    ]),\n    ...mapMutations(\"modal\", [\"SHOW_MODAL\", \"HIDE_MODAL\"]),\n    editRiesgoAutomotor(id) {\n      this.getRiesgoAutomotor(id);\n      this.SHOW_MODAL(true);\n    },\n    openDeleteModal(id) {\n      this.idSelected = id;\n      this.modalDelete = true;\n    },\n    deleteRiesgoAut() {\n      this.deleteRiesgoAutomotor(this.idSelected);\n      this.modalDelete = false;\n      this.idSelected = \"\";\n    }\n  },\n  created() {}\n};\n</script>\n\n<style>\n</style>"]}]}