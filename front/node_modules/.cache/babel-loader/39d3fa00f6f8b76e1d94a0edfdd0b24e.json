   textCompleto(nro) {\n      return nro === 1 || nro === true ? \"SI\" : \"NO\";\n    },\n    async cargarLocalidades() {\n      const resp = await http.get(\"/localidades\");\n      this.localidades = resp.data;\n    },\n    formatDate(date) {\n      if (date) {\n        return moment(date).format(\"DD/MM/YYYY\");\n      } else {\n        return \"\";\n      }\n    },\n  },\n  computed: {\n    fechaFormateada: {\n      set: function () {\n        return this.formatDate(this.cliente.nacimiento);\n      },\n      get: function () {\n        return this.formatDate(this.cliente.nacimiento);\n      },\n    },\n  },\n};\n"]}]}