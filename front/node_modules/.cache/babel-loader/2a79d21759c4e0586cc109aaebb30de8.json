AA,WAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAHA,EAIA;AAAA,QAAA,IAAA,EAAA,UAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAJA,EAKA;AAAA,QAAA,IAAA,EAAA,cAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OALA,EAMA;AAAA,QAAA,IAAA,EAAA,UAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OANA,EAOA;AAAA,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,QAAA,EAAA,KAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAPA,CAAA;AASA;AAbA,IAVA;AAyBA,EAAA,OAAA,oBACA,UAAA,CAAA,WAAA,EAAA,CACA,eADA,EAEA,cAFA,EAGA,iBAHA,CAAA,CADA,MAMA,YAAA,CAAA,OAAA,EAAA,CAAA,aAAA,EAAA,aAAA,CAAA,CANA;AAOA,IAAA,aAPA,yBAOA,EAPA,EAOA;AACA,WAAA,YAAA,CAAA,EAAA;AACA,WAAA,WAAA,CAAA,IAAA;AACA,KAVA;AAWA,IAAA,eAXA,2BAWA,EAXA,EAWA;AACA,WAAA,UAAA,GAAA,EAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,KAdA;AAeA,IAAA,SAfA,uBAeA;AACA,WAAA,YAAA,CAAA,KAAA,UAAA;AACA,WAAA,WAAA,GAAA,KAAA;AACA,WAAA,UAAA,GAAA,EAAA;AACA;AAnBA,IAzBA;AA8CA,EAAA,OA9CA,qBA8CA;AACA,SAAA,aAAA;AACA;AAhDA,CAAA","sourcesContent":["<template>\n  <v-card class=\"mt-0 mx-4 pa-3\">\n    <v-card-title>\n      Siniestros\n      <v-spacer></v-spacer>\n      <!-- <v-text-field\n        v-model=\"search\"\n        append-icon=\"mdi-magnify\"\n        label=\"Search\"\n        single-line\n        hide-details\n        v-uppercase\n      ></v-text-field> -->\n      <v-dialog\n        @click:outside=\"HIDE_MODAL3(false)\"\n        :value=\"modal3\"\n        max-width=\"60%\"\n      >\n        <modal-siniestros />\n      </v-dialog>\n    </v-card-title>\n    <v-data-table\n      class=\"pa-2\"\n      :headers=\"headers\"\n      :items-per-page=\"5\"\n      :items=\"siniestros\"\n      :search=\"search\"\n      multi-sort\n      :loading=\"loading\"\n    >\n      <template v-slot:[`item.poliza_numero`]=\"{ item }\">\n        {{\n          item.polizas.numero\n        }}\n      </template>\n      <template v-slot:[`item.asegurado`]=\"{ item }\">\n        {{\n          item.polizas.clientes.apellido + \" \" + item.polizas.clientes.nombre\n        }}\n      </template>\n      <template v-slot:[`item.compania`]=\"{ item }\">\n        {{\n          item.polizas.companias.nombre\n        }}\n      </template>\n      <template v-slot:[`item.fechaSiniestro`]=\"{ item }\">\n        {{\n          formatDate(item.fecha_siniestro)\n        }}\n      </template>\n      <template v-slot:[`item.fecha_completo`]=\"{ item }\">\n        {{\n          item.fecha_completo == null ? \"NO\" : \"SI\"\n        }}\n      </template>\n      <template v-slot:[`item.actions`]=\"{ item }\">\n        <v-icon\n          small\n          @click=\"editSiniestro(item.id)\"\n          class=\"mr-2\"\n          color=\"success\"\n        >\n          mdi-pencil\n        </v-icon>\n        <v-icon\n          class=\"ml-2\"\n          small\n          @click=\"openDeleteModal(item.id)\"\n          color=\"error\"\n        >\n          mdi-close\n        </v-icon>\n      </template>\n    </v-data-table>\n    <v-dialog\n      :retain-focus=\"false\"\n      max-width=\"30%\"\n      v-model=\"modalDelete\"\n    >\n      <v-card class=\"pa-4\">\n        <v-card-text>\n          <span>Esta seguro que desea eliminar este Siniestro?</span>\n        </v-card-text>\n        <v-card-actions class=\"py-0 pt-3 pr-6 d-flex justify-end\">\n          <v-btn\n            dark\n            color=\"red\"\n            @click=\"modalDelete = false\"\n          >Cancelar</v-btn>\n          <v-btn\n            class=\"ml-4\"\n            dark\n            color=\"success\"\n            @click=\"deleteSin\"\n          >Confirmar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-card>\n</template>\n\n<script>\nimport { mapState, mapActions, mapMutations } from \"vuex\";\nimport ModalSiniestros from \"./ModalSiniestros\";\nimport { helpers } from \"../../../helpers\";\n\nexport default {\n  components: {\n    ModalSiniestros\n  },\n  mixins: [helpers],\n  data: () => ({\n    search: \"\",\n    idSelected: \"\",\n    modalDelete: false\n  }),\n  computed: {\n    ...mapState(\"modal\", [\"modal3\", \"edicion3\"]),\n    ...mapState(\"siniestro\", [\"siniestros\", \"loading\"]),\n    headers() {\n      return [\n        { text: \"Póliza Nro.\", value: \"poliza_numero\" },\n        { text: \"Numero\", value: \"numero_siniestro\" },\n        { text: \"Asegurado\", value: \"asegurado\" },\n        { text: \"Compañia\", value: \"compania\" },\n        { text: \"F. Siniestro\", value: \"fechaSiniestro\" },\n        { text: \"Completo\", value: \"fecha_completo\" },\n        { text: \"Actions\", value: \"actions\", sortable: false, align: \"right\" }\n      ];\n    }\n  },\n  methods: {\n    ...mapActions(\"siniestro\", [\n      \"getSiniestros\",\n      \"getSiniestro\",\n      \"deleteSiniestro\"\n    ]),\n    ...mapMutations(\"modal\", [\"SHOW_MODAL3\", \"HIDE_MODAL3\"]),\n    editSiniestro(id) {\n      this.getSiniestro(id);\n      this.SHOW_MODAL3(true);\n    },\n    openDeleteModal(id) {\n      this.idSelected = id;\n      this.modalDelete = true;\n    },\n    deleteSin() {\n      this.deleteEndoso(this.idSelected);\n      this.modalDelete = false;\n      this.idSelected = \"\";\n    }\n  },\n  created() {\n    this.getSiniestros();\n  }\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/views/pages/Siniestros"}]}