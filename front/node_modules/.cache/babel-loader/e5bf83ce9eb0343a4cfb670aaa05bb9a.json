liza.riesgo_automotor\"\n      :search=\"search\"\n      multi-sort\n    >\n      <template v-slot:[`item.actions`]=\"{ item }\">\n        <v-icon\n          small\n          @click=\"editRiesgoAutomotor(item)\"\n          class=\"mr-2\"\n          color=\"success\"\n        >\n          mdi-pencil\n        </v-icon>\n        <v-icon\n          class=\"ml-2\"\n          small\n          @click=\"openDeleteModal(item.id)\"\n          color=\"error\"\n        >\n          mdi-close\n        </v-icon>\n      </template>\n    </v-data-table>\n    <v-dialog\n      :retain-focus=\"false\"\n      max-width=\"30%\"\n      v-model=\"modalDelete\"\n    >\n      <v-card class=\"pa-4\">\n        <v-card-text>\n          <span>Esta seguro que desea eliminar este Riesgo?</span>\n        </v-card-text>\n        <v-card-actions class=\"py-0 pt-3 pr-6 d-flex justify-end\">\n          <v-btn\n            dark\n            color=\"red\"\n            @click=\"modalDelete = false\"\n          >Cancelar</v-btn>\n          <v-btn\n            class=\"ml-4\"\n            dark\n            color=\"success\"\n            @click=\"deleteRiesgoAut\"\n          >Confirmar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-card>\n</template>\n\n<script>\nimport { mapState, mapActions, mapMutations } from \"vuex\";\nimport ModalRiesgoAutomotor from \"./ModalRiesgoAutomotor/ModalRiesgoAutomotor\";\nimport { helpers } from \"../../../../../helpers\";\n\nexport default {\n  components: {\n    ModalRiesgoAutomotor\n  },\n  mixins: [helpers],\n  data: () => ({\n    search: \"\",\n    idSelected: \"\",\n    modalDelete: false\n  }),\n  computed: {\n    ...mapState(\"modal\", [\"modal_ra\"]),\n    ...mapState(\"poliza\", [\"poliza\"]),\n    headers() {\n      return [\n        // { text: \"Tipo\", value: \"riesgo_automotor.tipo\" },\n        { text: \"Marca\", value: \"marca.nombre\" },\n        { text: \"Patente\", value: \"patente\" },\n        {\n          text: \"Suma Asegurada\",\n          value: \"valor_total\"\n        },\n        { text: \"Cobertura\", value: \"cobertura.nombre\" },\n        { text: \"Actions\", value: \"actions\", sortable: false, align: \"right\" }\n      ];\n    }\n  },\n  methods: {\n    ...mapActions(\"riesgo_automotor\", [\n      \"getRiesgoAutomotor\",\n      \"deleteRiesgoAutomotor\"\n    ]),\n    ...mapActions(\"anio\", [\"getAnios\"]),\n    ...mapActions(\"marca\", [\"getMarcas\"]),\n    ...mapActions(\"modelo\", [\"getModelos\"]),\n    ...mapActions(\"version\", [\"getVersiones\"]),\n    ...mapMutations(\"modal\", [\"SHOW_MODAL_RA\", \"HIDE_MODAL_RA\"]),\n    ...mapMutations(\"version\", [\"UPDATE_MODELO_ID\"]),\n    ...mapMutations(\"riesgo\", [\n      \"SET_RIESGO_AUTOMOTOR\",\n      \"RESET_RIESGO_AUTOMOTOR\"\n    ]),\n    editRiesgoAutomotor(item) {\n      this.SET_RIESGO_AUTOMOTOR(item);\n      this.SHOW_MODAL_RA(true);\n    },\n    create() {\n      this.RESET_RIESGO_AUTOMOTOR();\n      this.SHOW_MODAL_RA(false);\n    },\n    openDeleteModal(id) {\n      this.idSelected = id;\n      this.modalDelete = true;\n    },\n    deleteRiesgoAut() {\n      this.deleteRiesgoAutomotor(this.idSelected);\n      this.modalDelete = false;\n      this.idSelected = \"\";\n    }\n  },\n  created() {\n    this.getAnios();\n    this.getMarcas();\n    this.getModelos();\n    this.getVersiones();\n  }\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/views/pages/Polizas/Riesgos/Automotor"}]}