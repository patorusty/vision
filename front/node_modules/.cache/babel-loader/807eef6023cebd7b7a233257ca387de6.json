A,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAHA,EAIA;AAAA,QAAA,IAAA,EAAA,UAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAJA,EAKA;AAAA,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,KAAA,EAAA,SAAA;AAAA,QAAA,QAAA,EAAA,KAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OALA,CAAA;AAOA;AAXA,IAVA;AAuBA,EAAA,OAAA,oBACA,UAAA,CAAA,QAAA,EAAA,CAAA,WAAA,EAAA,cAAA,CAAA,CADA,MAEA,YAAA,CAAA,OAAA,EAAA,CAAA,aAAA,EAAA,aAAA,CAAA,CAFA;AAGA,IAAA,UAHA,sBAGA,EAHA,EAGA;AACA,WAAA,SAAA,CAAA,EAAA;AACA,WAAA,WAAA,CAAA,IAAA;AACA,KANA;AAOA,IAAA,eAPA,2BAOA,EAPA,EAOA;AACA,WAAA,UAAA,GAAA,EAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,KAVA;AAWA,IAAA,SAXA,uBAWA;AACA,WAAA,YAAA,CAAA,KAAA,UAAA;AACA,WAAA,WAAA,GAAA,KAAA;AACA,WAAA,UAAA,GAAA,EAAA;AACA;AAfA;AAvBA,CAAA","sourcesContent":["<template>\n  <v-card class=\"mt-0 mx-4 pa-3\">\n    <v-card-title>\n      Endosos\n      <v-spacer></v-spacer>\n      <v-spacer></v-spacer>\n      <v-btn\n        color=\"primary\"\n        @click=\"SHOW_MODAL2(false)\"\n        dark\n      >Crear</v-btn>\n      <v-dialog\n        @click:outside=\"HIDE_MODAL2(false)\"\n        :value=\"modal2\"\n        max-width=\"40%\"\n      >\n        <modal-endosos />\n      </v-dialog>\n    </v-card-title>\n    <v-data-table\n      class=\"pa-2\"\n      :headers=\"headers\"\n      :items-per-page=\"5\"\n      :items=\"endosos\"\n      :search=\"search\"\n      multi-sort\n      :loading=\"loading\"\n    >\n      <template v-slot:[`item.fecha`]=\"{ item }\">\n        {{\n          formatDate(item.fecha_solicitud)\n        }}\n      </template>\n      <template v-slot:[`item.tipo`]=\"{ item }\">\n        {{\n          item.tipo_endoso.nombre\n        }}\n      </template>\n      <template v-slot:[`item.endosoCompleto`]=\"{ item }\">\n        {{\n          textCompleto(item.completo)\n        }}\n      </template>\n      <template v-slot:[`item.actions`]=\"{ item }\">\n        <v-icon\n          small\n          @click=\"editEndoso(item.id)\"\n          class=\"mr-2\"\n          color=\"success\"\n        >\n          mdi-pencil\n        </v-icon>\n        <v-icon\n          class=\"ml-2\"\n          small\n          @click=\"openDeleteModal(item.id)\"\n          color=\"error\"\n        >\n          mdi-close\n        </v-icon>\n      </template>\n    </v-data-table>\n    <v-dialog\n      :retain-focus=\"false\"\n      max-width=\"30%\"\n      v-model=\"modalDelete\"\n    >\n      <v-card class=\"pa-4\">\n        <v-card-text>\n          <span>Esta seguro que desea eliminar este Endoso?</span>\n        </v-card-text>\n        <v-card-actions class=\"py-0 pt-3 pr-6 d-flex justify-end\">\n          <v-btn\n            dark\n            color=\"red\"\n            @click=\"modalDelete = false\"\n          >Cancelar</v-btn>\n          <v-btn\n            class=\"ml-4\"\n            dark\n            color=\"success\"\n            @click=\"deleteEnd\"\n          >Confirmar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-card>\n</template>\n\n<script>\nimport { mapState, mapActions, mapMutations } from \"vuex\";\nimport ModalEndosos from \"./ModalEndosos\";\nimport { helpers } from \"../../../../helpers\";\n\nexport default {\n  components: {\n    ModalEndosos\n  },\n  mixins: [helpers],\n  data: () => ({\n    search: \"\",\n    idSelected: \"\",\n    modalDelete: false\n  }),\n  computed: {\n    ...mapState(\"modal\", [\"modal2\", \"edicion2\"]),\n    ...mapState(\"endoso\", [\"endosos\", \"tipo_endosos\", \"loading\"]),\n    headers() {\n      return [\n        { text: \"Fecha\", value: \"fecha\" },\n        { text: \"Tipo\", value: \"tipo\" },\n        { text: \"Detalle\", value: \"detalle_endoso.nombre\" },\n        { text: \"Completo\", value: \"endosoCompleto\" },\n        { text: \"Actions\", value: \"actions\", sortable: false, align: \"right\" }\n      ];\n    }\n  },\n  methods: {\n    ...mapActions(\"endoso\", [\"getEndoso\", \"deleteEndoso\"]),\n    ...mapMutations(\"modal\", [\"SHOW_MODAL2\", \"HIDE_MODAL2\"]),\n    editEndoso(id) {\n      this.getEndoso(id);\n      this.SHOW_MODAL2(true);\n    },\n    openDeleteModal(id) {\n      this.idSelected = id;\n      this.modalDelete = true;\n    },\n    deleteEnd() {\n      this.deleteEndoso(this.idSelected);\n      this.modalDelete = false;\n      this.idSelected = \"\";\n    }\n  }\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/views/pages/Polizas/Endosos"}]}