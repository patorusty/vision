A,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA,IAAA,KAAA,CAAA,cAAA;AAAA,OAAA,CAAA;AACA,WAAA,UAAA,CAAA,MAAA,CAAA,CAAA,CAAA;AACA;AAhBA;AARA,CAAA","sourcesContent":["<template>\n  <v-card>\n    <v-card-title>\n      <span>Crear Riesgo</span>\n    </v-card-title>\n    <v-card-text>\n      <v-container>\n        <v-row>\n          <v-col class=\" mt-0 py-0\">\n            <v-select\n              v-model=\"tipo_riesgo_id\"\n              :items=\"tipo_riesgos\"\n              item-value=\"id\"\n              item-text=\"nombre\"\n              label=\"Elegir tipo de riesgo\"\n              class=\"mr-5\"\n            ></v-select>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-card-text>\n    <v-card-actions class=\"d-flex justify-end\">\n      <!-- <v-btn\n        class=\"mb-2\"\n        color=\"red\"\n        text\n        @click=\"close\"\n      >Cerrar</v-btn> -->\n      <v-btn\n        class=\"mb-2\"\n        color=\"green\"\n        @click=\"create\"\n        text\n        native-type=\"submit\"\n      >Crear</v-btn>\n    </v-card-actions>\n  </v-card>\n</template>\n<script>\nimport { mapState, mapMutations } from \"vuex\";\nexport default {\n  data: () => ({\n    tipo_riesgo_id: null\n  }),\n  computed: {\n    ...mapState(\"poliza\", [\"tipo_riesgos\"]),\n    ...mapState(\"modal\", [\"modal\", \"modal_ra\", \"modal_or\"])\n  },\n  methods: {\n    ...mapMutations(\"modal\", [\"HIDE_MODAL\", \"SHOW_MODAL_RA\", \"SHOW_MODAL_OR\"]),\n    ...mapMutations(\"poliza\", [\"SET_RIESGO\"]),\n    create() {\n      this.setTipoRiesgo();\n      this.HIDE_MODAL(false);\n      if (this.tipo_riesgo_id == 1) {\n        this.SHOW_MODAL_RA(true);\n      } else {\n        this.SHOW_MODAL_OR(true);\n      }\n    },\n    close() {},\n    setTipoRiesgo() {\n      var riesgo = this.tipo_riesgos.filter(i => i.id == this.tipo_riesgo_id);\n      this.SET_RIESGO(riesgo[0]);\n    }\n  }\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/views/pages/Polizas/Riesgos"}]}