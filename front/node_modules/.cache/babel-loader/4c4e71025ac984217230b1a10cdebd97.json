t:[`item.completo`]=\"{ item }\">\n        {{\n          textCompleto(item.completo)\n        }}\n      </template>\n      <template v-slot:[`item.actions`]=\"{ item }\">\n        <v-icon\n          small\n          @click=\"editEndoso(item.id)\"\n          class=\"mr-2\"\n          color=\"success\"\n        >\n          mdi-pencil\n        </v-icon>\n        <v-icon\n          class=\"ml-2\"\n          small\n          @click=\"openDeleteModal(item.id)\"\n          color=\"error\"\n        >\n          mdi-close\n        </v-icon>\n      </template>\n    </v-data-table>\n    <v-dialog\n      :retain-focus=\"false\"\n      max-width=\"30%\"\n      v-model=\"modalDelete\"\n    >\n      <v-card class=\"pa-4\">\n        <v-card-text>\n          <span>Esta seguro que desea eliminar este Endoso?</span>\n        </v-card-text>\n        <v-card-actions class=\"py-0 pt-3 pr-6 d-flex justify-end\">\n          <v-btn\n            dark\n            color=\"red\"\n            @click=\"modalDelete = false\"\n          >Cancelar</v-btn>\n          <v-btn\n            class=\"ml-4\"\n            dark\n            color=\"success\"\n            @click=\"deleteEnd\"\n          >Confirmar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-card>\n</template>\n\n<script>\nimport { mapState, mapActions, mapMutations } from \"vuex\";\nimport ModalEndosos from \"./ModalEndosos\";\nimport { helpers } from \"../../../helpers\";\n\nexport default {\n  components: {\n    ModalEndosos\n  },\n  mixins: [helpers],\n  data: () => ({\n    search: \"\",\n    idSelected: \"\",\n    modalDelete: false\n  }),\n  computed: {\n    ...mapState(\"modal\", [\"modal2\", \"edicion2\"]),\n    ...mapState(\"endoso\", [\"endosos\", \"tipo_endosos\", \"loading\"]),\n    headers() {\n      return [\n        { text: \"Póliza Nro.\", value: \"poliza.numero\" },\n        { text: \"Tipo\", value: \"tipo\" },\n        { text: \"Asegurado\", value: \"asegurado\" },\n        { text: \"Compañia\", value: \"compania\" },\n        { text: \"F. Solicitud\", value: \"fecha_solicitud\" },\n        { text: \"Completo\", value: \"completo\" },\n        { text: \"Actions\", value: \"actions\", sortable: false, align: \"right\" }\n      ];\n    }\n  },\n  methods: {\n    ...mapActions(\"endoso\", [\n      \"getEndosos\",\n      \"getEndoso\",\n      \"deleteEndoso\",\n      \"getTipoEndosos\",\n      \"getDetalleEndosos\"\n    ]),\n    ...mapMutations(\"modal\", [\"SHOW_MODAL2\", \"HIDE_MODAL2\"]),\n    editEndoso(id) {\n      this.getEndoso(id);\n      this.SHOW_MODAL2(true);\n    },\n    openDeleteModal(id) {\n      this.idSelected = id;\n      this.modalDelete = true;\n    },\n    deleteEnd() {\n      this.deleteEndoso(this.idSelected);\n      this.modalDelete = false;\n      this.idSelected = \"\";\n    }\n  },\n  created() {\n    this.getEndosos();\n    this.getTipoEndosos();\n    this.getDetalleEndosos();\n  }\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/views/pages/Endosos"}]}